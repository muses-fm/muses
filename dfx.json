{
  "canisters": {
    "artist": {
      "main": "src/backend/artist/main.mo",
      "type": "motoko"
    },
    "curator": {
      "main": "src/backend/curator/main.mo",
      "type": "motoko"
    },
    "frontend": {
      "dependencies": [
        "artist",
        "curator"
      ],
      "frontend": {
        "entrypoint": "src/frontend/public/index.js",
        "output": "dist/frontend"
      },
      "source": [
        "src/frontend/assets",
        "dist/frontend/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.6.16",
  "networks": {
    "ic": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
